{
  "properties": {
    "searchWhat": {
      "type": "string",
      "pattern": "^projects$"
    },
    "searchBy": {
      "type": "string",
      "pattern": "^projectId$"
    },
    "projectIdFrom": {
      "type": "integer",
      "minimum": 1
    },
    "projectIdTo": {
      "type": "integer",
      "minimum": 1
    }
  },
  "required": ["projectIdFrom", "projectIdTo", "searchWhat", "searchBy"],
  "additionalProperties": false
}
