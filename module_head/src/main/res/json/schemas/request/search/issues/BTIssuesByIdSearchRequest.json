{
  "properties": {
    "searchWhat": {
      "type": "string",
      "pattern": "^issues$"
    },
    "searchBy": {
      "type": "string",
      "pattern": "^issueId$"
    },
    "issueIdFrom": {
      "type": "integer",
      "minimum": 0
    },
    "issueIdTo": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["issueIdFrom", "issueIdTo", "searchBy", "searchWhat"],
  "additionalProperties": false
}
