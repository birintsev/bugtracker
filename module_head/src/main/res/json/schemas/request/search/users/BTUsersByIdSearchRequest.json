{
  "properties": {
    "searchWhat": {
      "type": "string",
      "pattern": "^users$"
    },
    "searchBy": {
      "type": "string",
      "pattern": "^userId$"
    },
    "userIdFrom": {
      "type": "integer",
      "minimum": 1
    },
    "userIdTo": {
      "type": "integer",
      "minimum": 1
    }
  },
  "required": ["userIdFrom", "userIdTo", "searchWhat", "searchBy"],
  "additionalProperties": false
}
